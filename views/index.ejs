  <style>
     .premiere-banner {
      display: flex;
      align-items: center;
      padding: 20px;
  
    }
    .premiere-banner img {
      height: 40px;
      margin-right: 15px;
    }
    .movie-card {
      background-color: #2b3148;
      border: none;
      border-radius: 10px;
      overflow: hidden;
      transition: transform 0.3s;
    }
    .movie-card:hover {
      transform: scale(1.05);
    }
    .movie-card img {
      height: 280px;
      object-fit: cover;
    }
    .premiere-tag {
      position: absolute;
      bottom: 0;
      left: 0;
      background-color: #e83e8c;
      color: white;
      font-size: 0.9rem;
      font-weight: bold;
      padding: 4px 10px;
      border-top-right-radius: 6px;
    }
    .movie-title {
      font-weight: bold;
      margin-top: 8px;
      font-size: 1rem;
    }
    .movie-lang {
      color: #bbb;
      font-size: 0.9rem;
    }
    .scroll-container {
      
      display: flex;
      gap: 50px;
      overflow-x: auto;
      padding-bottom: 10px;
      scrollbar-width: none; /* Firefox */
    }
    .scroll-container::-webkit-scrollbar {
      display: none; /* Chrome, Safari */
    }
    .logo-ticket { font-weight: 800; font-size: 1.25rem; letter-spacing: 0.5px; }
    .logo-red { color: #e53935; }
    .hover-dark:hover { color:#e53935 !important; }
    .hero-wrapper { position: relative; background: linear-gradient(90deg,#0b0d19,#141a2e); border-radius: 20px; overflow: hidden; }
    .hero-wrapper img { display:block; width:100%; height:auto; }
     .event-card {
      border-radius: 12px;
      overflow: hidden;
      transition: transform 0.3s;
    }
    .event-card:hover {
      transform: scale(1.05);
    }
    .event-card img {
    
      object-fit: cover;
    }
    .event-title {
      font-size: 1.2rem;
      font-weight: bold;
    }
    .event-count {
      color: #666;
    }
    .s{
       background-color: #2b3148;
      color: white;
    }
      .music-section {
      padding: 30px;
    }
    .music-card {
      border: none;
      border-radius: 12px;
      overflow: hidden;
      background-color: white;
      transition: transform 0.3s;
      min-width: 220px;
    }
    .music-card:hover {
      transform: scale(1.05);
    }
    .music-card img {
      border-radius: 12px 12px 0 0;
      height: 320px;
      object-fit: cover;
      width: 100%;
    }
    .event-date {
      position: absolute;
      bottom: 0;
      left: 0;
      background-color: black;
      color: white;
      font-size: 0.9rem;
      padding: 4px 12px;
      border-top-right-radius: 8px;
    }
    .scroll-container {
      display: flex;
      gap: 20px;
      overflow-x: auto;
      padding-bottom: 10px;
      scrollbar-width: none;
    }
    .scroll-container::-webkit-scrollbar {
      display: none;
    }
    .event-title {
      font-weight: bold;
      font-size: 1rem;
      margin-top: 10px;
    }
    .event-venue {
      color: #555;
      font-size: 0.9rem;
    }
    .event-type {
