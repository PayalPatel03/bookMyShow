<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title>BookMyShow</title>
    <link rel="stylesheet" href="/assets/css/home.css" />
    <link
      href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.7/dist/css/bootstrap.min.css"
      rel="stylesheet"
      integrity="sha384-LN+7fdVzj6u52u30Kp6M/trliBMCMKTyK833zpbD+pXdCLuTusPj697FH4R/5mcr"
      crossorigin="anonymous"
    />
    <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/7.0.0/css/all.min.css" integrity="sha512-DxV+EoADOkOygM4IR9yXP8Sb2qwgidEmeqAEmDKIOfPRQZOWbXCzLC6vjbZyy0vPisbH2SyW27+ddLVCN+OMzQ==" crossorigin="anonymous" referrerpolicy="no-referrer" />
  </head>
  <body>
    <!--navbar start-->
     <nav class="navbar navbar-expand-lg bg-white border-bottom sticky-top">
      <div class="container-fluid px-3 px-md-4">
        <!-- Brand -->
        <a class="navbar-brand d-flex align-items-center gap-2" href="#">
          <span class="logo-ticket"
            ><img
              src="data:image/png;base64,"
              height="60"
              alt=""
          /></span>
        </a>

        <!-- Search -->
        <form class="d-none d-lg-flex flex-grow-1 mx-3">
          <div class="input-group">
            <span class="input-group-text bg-white border-end-0"
              ><i class="fa-solid fa-magnifying-glass"></i
            ></span>
            <input
              class="form-control border-start-0"
              type="search"
              placeholder="Search for Movies, Events, Plays, Sports and Activities"
            />
          </div>
        </form>

        <!-- Right side -->
        <div class="d-flex align-items-center gap-3">
          <div class="dropdown">
            <button
              class="btn btn-sm btn-outline-secondary dropdown-toggle"
              data-bs-toggle="dropdown"
            >
              <i class="fa-solid fa-location-dot me-1"></i> Navsari
            </button>
            <ul class="dropdown-menu dropdown-menu-end">
              <li><a class="dropdown-item" href="#">Surat</a></li>
              <li><a class="dropdown-item" href="#">Vadodara</a></li>
              <li><a class="dropdown-item" href="#">Ahmedabad</a></li>
            </ul>
          </div>
          <div class="dropdown">
            <button
              class="btn btn-sm btn-outline-dark rounded-circle p-2 d-flex align-items-center justify-content-center"
              data-bs-toggle="dropdown"
            >
              <i class="fa-regular fa-user"></i>
            </button>
            <ul class="dropdown-menu dropdown-menu-end">
              <li class="dropdown-header small text-muted px-3">
                Hi, Payal Patel
              </li>
              <li><hr class="dropdown-divider" /></li>
              <li><a class="dropdown-item" href="#">My Bookings</a></li>
              <li><a class="dropdown-item" href="#">Profile</a></li>
              <li><a class="dropdown-item" href="#">Logout</a></li>
            </ul>
          </div>
        </div>
      </div>

      <!-- Mobile Search -->
      <div class="container-fluid d-lg-none px-3 pb-2">
        <div class="input-group">
          <span class="input-group-text bg-white border-end-0"
            ><i class="fa-solid fa-magnifying-glass"></i
          ></span>
          <input
            class="form-control border-start-0"
            type="search"
            placeholder="Search for Movies, Events, Plays, Sports and Activities"
          />
        </div>
      </div>
    </nav>

    <!--navbar end-->
     <div class="bg-white border-bottom">
      <div
        class="container-fluid px-3 px-md-4 d-flex gap-4 align-items-center overflow-auto py-2 small"
      >
        <a
          href="#"
          class="text-decoration-none text-dark fw-semibold hover-dark"
          >Movies</a
        >
        <a
          href="#"
          class="text-decoration-none text-dark fw-semibold hover-dark"
          >Stream</a
        >
        <a
          href="#"
          class="text-decoration-none text-dark fw-semibold hover-dark"
          >Events</a
        >
        <a
          href="#"
          class="text-decoration-none text-dark fw-semibold hover-dark"
          >Plays</a
        >
        <a
          href="#"
          class="text-decoration-none text-dark fw-semibold hover-dark"
          >Sports</a
        >
        <a
          href="#"
          class="text-decoration-none text-dark fw-semibold hover-dark"
          >Activities</a
        >
        <div class="ms-auto d-none d-md-flex gap-4">
          <a href="#" class="text-decoration-none text-muted">ListYourShow</a>
          <a href="#" class="text-decoration-none text-muted">Corporates</a>
          <a href="#" class="text-decoration-none text-muted">Offers</a>
          <a href="#" class="text-decoration-none text-muted">Gift Cards</a>
        </div>
      </div>
    </div>

      <section class="container-fluid">
      <div class="hero-wrapper">
        <img
          src="https://assets-in-gm.bmscdn.com/promotions/cms/creatives/1755155608642_minecraftflashsaleweb.jpg"
         width="100%"
          alt="Hero banner"
        />
      </div>
    </section>
    
<!--2387284-->
    <!-- Hero-section -->
    

    <!-- cards section -->
    <section class="movie-card-section">
      <div class="container">
        <div class="movie-content">
          <div class="m-header d-flex justify-content-between align-items-center">
            <span class="text-white fw-medium fs-4">Now Showing</span>
            <a href="" class="text-decoration-none text-danger">
              View all 
              <i class="fa-solid fa-arrow-right ms-1"></i>
            </a>
          </div>
          <div class="movie-card mt-5 d-flex gap-5 flex-wrap">
            <% movie.map((val)=>{ %>
              <div class="card" style="width: 17rem;">
              <img src="<%= val.image%>" class="card-img-top p-2 rounded-4" alt="...">
              <div class="card-body text-white">
                <h5 class="card-title"><%= val.m_name %></h5>
                <p class="card-text">
                  <span>2025 |</span>
                  <span>Action | Thriller</span>
                  <span>| <%= val.m_time %></span>
                </p>
                <a href="/movie/<%= val.id%>" class="btn btn-danger rounded-pill">Buy Ticket</a>
              </div>
            </div>
            <% }) %>
            
          </div>
        </div>
      </div>
    </section>

      <div class="container p-0 mt-5">
      <img
        src="https://assets-in.bmscdn.com/discovery-catalog/collections/tr:w-1440,h-120/stream-leadin-web-collection-202210241242.png"
        class="img-fluid w-100 rounded"
        alt="BookMyShow Stream"
      />
    </div>

    <!-- Trailers -->
     <section class="trailer-card">
      <div class="container mt-5">
  <h2 class="text-white">Trailers</h2>

  <!-- Featured trailer -->
  <!-- <% if (trailer.length > 0) { %>
    <div class="mb-4">

      <iframe width="100%" height="480"
        src="<%= trailer[0].youtubeUrl %>"
        title="<%= trailer[0].title %>" frameborder="0"
        allowfullscreen class="p-4">
      </iframe>

      <h3 class="text-white mt-2"><%= trailer[0].title %></h3>
      <p class="text-secondary"><%= trailer[0].description %></p>
    </div>
  <% } %> -->

  <!-- Thumbnails -->
  <div class="d-flex gap-5 flex-wrap">
    <% trailer.forEach((trailer, i) => { %>
      <div class="card bg-dark text-white" style="width: 17rem;">
        <img src="<%= trailer.thumbnail %>" class="card-img-top p-2 rounded-4" alt="<%= trailer.title %>">
        <div class="card-body">
          <h5><%= trailer.title %></h5>
          <a href="<%= trailer.youtubeUrl %>" target="_blank" class="btn btn-danger">Watch</a>
        </div>
      </div>
    <% }) %>
  </div>
</div>
     </section>
     

    <!-- Footer -->
